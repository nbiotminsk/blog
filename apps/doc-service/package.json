{
  "name": "@nikolai/doc-service",
  "version": "0.1.0",
  "description": "Document generation microservice for template parsing and rendering with CarboneJS",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc --project tsconfig.json",
    "start": "node dist/server.js",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "document-generation",
    "carbone",
    "typescript",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "carbone": "^3.5.4",
    "docxtemplater": "^3.48.0",
    "dotenv": "^16.4.7",
    "express": "^4.19.2",
    "multer": "^2.0.2",
    "pizzip": "^3.1.7",
    "pino": "^8.19.0",
    "pino-pretty": "^11.0.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
